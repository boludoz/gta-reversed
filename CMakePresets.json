{
    "version": 4,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 23,
        "patch": 0
    },
    "include": ["./ConanPresets.json"],
    "configurePresets": [
        {
            "name": "default-unity",
            "inherits": "conan-default",
            "cacheVariables": {
                "GTASA_UNITY_BUILD": "ON",
                "GTASA_WITH_SDL3": "ON"
            }
        },
        {
            "name": "default",
            "inherits": "conan-default",
            "cacheVariables": {
                "GTASA_UNITY_BUILD": "OFF",
                "GTASA_WITH_SDL3": "ON"
            }
        },
        {
            "name": "default-unity-nosdl",
            "inherits": "conan-default",
            "cacheVariables": {
                "GTASA_UNITY_BUILD": "ON",
                "GTASA_WITH_SDL3": "OFF"
            }
        },
        {
            "name": "default-nosdl",
            "inherits": "conan-default",
            "cacheVariables": {
                "GTASA_UNITY_BUILD": "OFF",
                "GTASA_WITH_SDL3": "OFF"
            }
        }
    ]
}
